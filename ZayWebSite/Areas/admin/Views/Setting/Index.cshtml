@{
    ViewData["Title"] = "Index";
}
@model SettingGetDto
<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-lg-6 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>Email</th>
                                        <th>Phone</th>
                                        <th>Logo</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>@Model.Email</td>
                                        <td>@Model.Phone</td>
                                        <td>@Model.Logo</td>
                                        <td style="color:black" > 
                                            <a asp-area="admin" asp-controller="setting" asp-action="update">
                                                <i class="mdi mdi-border-color "></i>
                                            </a>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

