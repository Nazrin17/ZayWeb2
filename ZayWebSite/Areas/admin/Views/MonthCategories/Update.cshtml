
@{
    ViewData["Title"] = "Update";
}
@model CategoriesMonthUpdateDto
<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-md-6 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <form class="forms-sample" method="post" enctype="multipart/form-data">
                            <div class="form-group">
                                <label asp-for="@Model.postDto.Title">title</label>
                                <input asp-for="@Model.postDto.Title" value="@Model.getDto.Title"class="form-control">
                            </div>
                            <div class="form-group">
                                <label asp-for="@Model.postDto.Description">description</label>
                                <input asp-for="@Model.postDto.Description" value="@Model.getDto.Description">
                            </div>
                            <button type="submit" class="btn btn-primary me-2">Submit</button>
                            <button class="btn btn-light">Cancel</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

